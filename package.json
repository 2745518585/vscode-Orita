{
    "name": "orita",
    "displayName": "Orita",
    "description": "Useful OI Tools",
    "version": "0.0.14",
    "publisher": "2745518585",
    "engines": {
        "vscode": "^1.75.0"
    },
    "icon": "vscode-Orita.png",
    "categories": [
        "Other"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/2745518585/vscode-Orita.git"
    },
    "activationEvents": [
        "onLanguage:cpp"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "orita.compile-run",
                "title": "Orita: Compile run"
            },
            {
                "command": "orita.compile",
                "title": "Orita: Compile"
            },
            {
                "command": "orita.run",
                "title": "Orita: Run"
            },
            {
                "command": "orita.add-file",
                "title": "Orita: add file"
            },
            {
                "command": "orita.add-file1",
                "title": "Orita: add file1"
            },
            {
                "command": "orita.add-file2",
                "title": "Orita: add file2"
            },
            {
                "command": "orita.add-file3",
                "title": "Orita: add file3"
            },
            {
                "command": "orita.show-run-data",
                "title": "Orita: Show run data"
            },
            {
                "command": "orita.compare-run-data",
                "title": "Orita: Compare run data"
            },
            {
                "command": "orita.enter-address",
                "title": "Orita: Enter address"
            }
        ],
        "keybindings": [
            {
                "command": "orita.compile-run",
                "key": "f6"
            },
            {
                "command": "orita.compile",
                "key": "ctrl+f6"
            },
            {
                "command": "orita.run",
                "key": "alt+f6"
            },
            {
                "command": "orita.add-file",
                "key": "f7"
            },
            {
                "command": "orita.add-file1",
                "key": "ctrl+f7"
            },
            {
                "command": "orita.add-file2",
                "key": "alt+f7"
            },
            {
                "command": "orita.add-file3",
                "key": "shift+f7"
            },
            {
                "command": "orita.show-run-data",
                "key": "ctrl+shift+f7"
            },
            {
                "command": "orita.compare-run-data",
                "key": "ctrl+alt+f7"
            },
            {
                "command": "orita.enter-address",
                "key": "f8"
            }
        ],
        "configuration": {
            "title": "Orita",
            "properties": {
                "orita.enable_add_include_path": {
                    "type": "boolean",
                    "default": false,
                    "description": "Add orita path to include path"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "webpack",
        "watch": "webpack --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "pretest": "npm run compile-tests && npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^8.0.1",
        "@types/mocha": "^10.0.1",
        "@types/node": "16.x",
        "@types/vscode": "^1.75.0",
        "@typescript-eslint/eslint-plugin": "^5.49.0",
        "@typescript-eslint/parser": "^5.49.0",
        "@vscode/test-electron": "^2.2.2",
        "eslint": "^8.33.0",
        "glob": "^8.1.0",
        "mocha": "^10.1.0",
        "ts-loader": "^9.4.2",
        "typescript": "^4.9.4",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1"
    },
    "dependencies": {
        "vsce": "^2.15.0"
    }
}